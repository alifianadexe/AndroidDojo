<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alifianadexe.androidtrainingdojo">
    
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme" >

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CardViewActivity"></activity>
        <activity android:name=".TabLayoutActivity"></activity>
        <activity android:name=".moreactivity.ActivityFragmentTwoA"></activity>
        <activity android:name=".moreactivity.ActivityFragmentTwoB"></activity>
        <activity android:name=".moreactivity.ActivityFragmentTwoC"></activity>
        <activity android:name=".RecycleViewActivity"></activity>
        <activity android:name=".MapsActivity"></activity>
    </application>

</manifest>